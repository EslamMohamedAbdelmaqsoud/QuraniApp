<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.home.tasbih.TasbihActivity">


        <TextView
            android:id="@+id/text_Tasbih"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:fontFamily="@font/poppins_bold"
            android:text="التسبيح"
            android:textAlignment="center"
            android:textColor="#F9B091"
            android:textSize="@dimen/_30ssp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/icon_back_TasbihActivity"
            android:layout_width="@dimen/_25sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_marginStart="@dimen/_60sdp"
            android:layout_marginTop="@dimen/_30sdp"
            android:layout_marginEnd="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/text_Tasbih"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/backss" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/_110sdp"
            android:layout_height="@dimen/_110sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:src="@drawable/a6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_Tasbih" />

        <Button
            android:id="@+id/button1_Tasbih1_TasbihActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_30sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:background="@drawable/custom_input0"
            android:drawableLeft="@drawable/baseline_arrow_back_ios_new_24"
            android:fontFamily="@font/poppins_bold"
            android:foreground="@drawable/effect"
            android:paddingLeft="@dimen/_15sdp"
            android:paddingTop="@dimen/_5sdp"
            android:text="سُبْحَانَ اللَّهِ"
            android:textAllCaps="false"
            android:textColor="#F9B091"
            android:textSize="@dimen/_24ssp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <Button
            android:id="@+id/button3_Tasbih3_TasbihActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:background="@drawable/custom_input0"
            android:drawableLeft="@drawable/baseline_arrow_back_ios_new_24"
            android:fontFamily="@font/poppins_bold"
            android:foreground="@drawable/effect"
            android:paddingLeft="@dimen/_15sdp"
            android:paddingTop="@dimen/_5sdp"
            android:text="الْحَمْدُ لِلَّهِ"
            android:textAllCaps="false"
            android:textColor="#F9B091"
            android:textSize="@dimen/_24ssp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2_Tasbih2_TasbihActivity" />

        <Button
            android:id="@+id/button2_Tasbih2_TasbihActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:background="@drawable/custom_input00"
            android:drawableLeft="@drawable/baseline_arrow_back_ios_new_244"
            android:fontFamily="@font/poppins_bold"
            android:foreground="@drawable/effect"
            android:paddingLeft="@dimen/_15sdp"
            android:paddingTop="@dimen/_5sdp"
            android:text="الْلَّهُ أَكْبَرُ"
            android:textAllCaps="false"
            android:textColor="#9D1DF2"
            android:textSize="@dimen/_24ssp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button1_Tasbih1_TasbihActivity" />

        <Button
            android:id="@+id/button4_Tasbih4_TasbihActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:background="@drawable/custom_input00"
            android:drawableLeft="@drawable/baseline_arrow_back_ios_new_244"
            android:fontFamily="@font/poppins_bold"
            android:foreground="@drawable/effect"
            android:paddingLeft="@dimen/_15sdp"
            android:paddingTop="@dimen/_5sdp"
            android:text="لَا إِلَهَ إِلَّا اللَّهُ"
            android:textAllCaps="false"
            android:textColor="#9D1DF2"
            android:textSize="@dimen/_24ssp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button3_Tasbih3_TasbihActivity" />

        <Button
            android:id="@+id/button5_Tasbih5_TasbihActivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_25sdp"
            android:layout_marginEnd="@dimen/_40sdp"
            android:background="@drawable/custom_input0"
            android:drawableLeft="@drawable/baseline_arrow_back_ios_new_24"
            android:fontFamily="@font/poppins_bold"
            android:foreground="@drawable/effect"
            android:paddingLeft="@dimen/_15sdp"
            android:paddingTop="@dimen/_5sdp"
            android:text="أَسْتَغْفِرُ اللَّهَ"
            android:textAllCaps="false"
            android:textColor="#F9B091"
            android:textSize="@dimen/_24ssp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button4_Tasbih4_TasbihActivity" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>